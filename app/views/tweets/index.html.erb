<h1>Feed for showing  tweets created by all users</h1>
<div>
<% @tweets.each do |tweet| %>
    <div class = "border-t"></div>
    <div class="rounded">
        <b><p>@<%= tweet.user.username %></p></b>
        <%= tweet.body %>
        <%= link_to "Show this tweet", tweet_path(tweet), method: :get, class: "text-blue-500 hover:text-blue-700 hover:underline cursor-pointer" %>
    </div>
<% end %>
</div>
<div>
    <% if user_signed_in? %>
        <%= button_to "Log out", destroy_user_session_path, method: :delete, class: "bg-gray-500 hover:bg-gray-700 text-black font-bold rounded" %>
    <% end %>
</div>
<%= link_to "View User Profiles", profiles_path, method: :get, class: "text-blue-500 hover:text-blue-700 hover:underline cursor-pointer" %>