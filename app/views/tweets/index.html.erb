<h1>Feed for showing  tweets created by all users</h1>
<div>
<% @tweets.each do |tweet| %>
    <div>
        <b><p>@<%= tweet.user.username %></p></b>
        <%= tweet.body %>
        <%= link_to "Show this tweet", tweet_path(tweet), method: :get %>
    </div>
<% end %>
</div>
<div>
    <% if user_signed_in? %>
        <%= button_to "Log out", destroy_user_session_path, method: :delete %>
    <% end %>
</div>
<%= link_to "View User Profiles", profiles_path, method: :get %>